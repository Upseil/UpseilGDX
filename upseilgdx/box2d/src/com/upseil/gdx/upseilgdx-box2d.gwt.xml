<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module>
	<source path="box2d">
       <exclude name="**/EdgeShapeBuilder.*" />
       <exclude name="**/SimpleEdgeShapeBuilder.*" />
       <exclude name="**/ChainedEdgeShapeBuilder.*" />
    </source>
	
	<extend-configuration-property name="gdx.reflect.include" value="com.upseil.gdx.box2d.event" />
	<extend-configuration-property name="gdx.reflect.include" value="com.upseil.gdx.box2d.action" />
	
	<extend-configuration-property name="artemis.reflect.include" value="com.upseil.gdx.box2d.system" />
	<extend-configuration-property name="artemis.reflect.include" value="com.upseil.gdx.box2d.component" />
</module>